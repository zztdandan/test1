{
  "name": "avuex-sms",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env --MOCK_LOGIN=true --API_PATH_REWRITE=/api/ vue-cli-service serve --open=true --port 8866",
    "dev-local": "cross-env --PROXY_API_IP=local --PROXY_API_PORT=8080 --MOCK_LOGIN=true vue-cli-service serve --open --port 8866",
    "dev-local8888": "cross-env --PROXY_API_IP=local --PROXY_API_PORT=8888 --MOCK_LOGIN=false vue-cli-service serve --open --port 8877",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "analyz": "npm_config_report=true npm run build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classlist-polyfill": "^1.2.0",
    "element-ui": "^2.4.5",
    "js-cookie": "^2.2.0",
    "nprogress": "^0.2.0",
    "script-loader": "^0.7.2",
    "vue": "^2.5.16",
    "vue-axios": "^2.1.2",
    "vue-i18n": "^8.7.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.5",
    "@vue/cli-service": "^3.1.4",
    "mockjs": "^1.0.1-beta3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
